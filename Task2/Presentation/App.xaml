<Application x:Class="Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Presentation"
             xmlns:ViewModels="clr-namespace:Presentation.ViewModels"
             xmlns:views="clr-namespace:Presentation.Views"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type  ViewModels:UserListViewModel}">
            <views:UserListView/>
        </DataTemplate>
        
        <DataTemplate DataType="{x:Type  ViewModels:CatalogListViewModel}">
            <views:CatalogListView/>
        </DataTemplate>

        <DataTemplate DataType="{x:Type  ViewModels:EventListViewModel}">
            <views:EventListView/>
        </DataTemplate>
    </Application.Resources>
</Application>
